#include <stdio.h>
void main()
{
  char ch;
  printf("enter any character:");
  scanf("%c",&ch);
  switch(ch)
  {
    case 'A':
    case 'a':
    case 'E':
    case 'e':
    case 'I':
    case 'i':
    case 'O':
    case 'o':
    case 'U':
    case 'u':
           printf("given character is a vowel");
           break;
    default:printf("given character is a consonant");
  }
}
