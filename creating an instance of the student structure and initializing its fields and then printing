#include<stdio.h>
int main()
{
    struct student
    {
        int sno;
        char sname[20];
        int marks;
    };
    struct student s1={493,"Nasreen",90};
    printf("the sno is %d",s1.sno);
    printf("\nthe sname is %s",s1.sname);
    printf("\nthe marks are %d",s1.marks);
    return 0;
}
