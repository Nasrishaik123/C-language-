#include <stdio.h>
#include<string.h>
int main()
{
    char a[20];
    printf("enter any word");
    gets(a);
    int len=strlen(a)-1;
    for(int i=0,j=len;i<=j;i++,j--)
    {
      if(a[i]!=a[j])
      {
                printf("not a pallindrome");
      return;
      }
    }
    printf("pallindrome");
    }
