#include <stdio.h>
#include<string.h>
void main() {
  char str1[53],str2[35];
  {
      printf("enter a password");
      fgets("str1,sizeof(string),stdin");
      printf("re enter a password");
      fgets("str2,sizeof(string),stdin");
      if(strcmp(str1,str2==0))
      {
          printf("password matched");
      }
      else
      {
          printf("mismached the password");
      }
      
  }
}
   
