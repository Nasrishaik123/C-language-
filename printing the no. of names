#include <stdio.h>
#include<string.h>
int main()
{
  char a[30][30];
  int n;
  printf("enter the no of names");
  scanf("%d",&n);
  for(int i=0;i<n;i++)
  {
      printf("enter the names",a[i]);
      scanf("%s",a[i]);
  }
  printf("the names are as follows");
  for(int i=0;i<n;i++)
  {
      printf("\n Name%d.%s",i+1,a[i]);
  }
}
